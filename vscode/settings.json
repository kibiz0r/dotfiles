{
  "shifty.shiftMode": "discovery",
  "shifty.colorThemes.ignoreLightColorThemes": true,
  "shifty.shiftInterval.shiftFontFamilyIntervalMin": 0,
  "git.countBadge": "off",
  "macros.list": {
    "openAndKeep": [
      "list.select",
      "workbench.action.keepEditor",
      "workbench.action.toggleSidebarVisibility"
    ],
    "mergeGroupsAndHideSidebar": [
      "workbench.action.joinAllGroups",
      "workbench.action.maximizeEditor",
      "workbench.action.closePanel"
    ],
    "focusFilesExplorerCollapsed": [
      "workbench.files.action.focusFilesExplorer",
      "workbench.files.action.collapseExplorerFolders",
    ],
    "switchToSettingsJson": [
      "workbench.action.closeActiveEditor",
      "workbench.action.openSettingsJson"
    ],
    "switchToSettingsUi": [
      "workbench.action.closeActiveEditor",
      "workbench.action.openSettings2"
    ],
    "switchToKeybindingsJson": [
      "workbench.action.closeActiveEditor",
      "workbench.action.openGlobalKeybindingsFile"
    ],
    "switchToKeybindingsUi": [
      "workbench.action.closeActiveEditor",
      "workbench.action.openGlobalKeybindings"
    ],
  },
  "editor.minimap.enabled": false,
  "editor.codeLens": true,
  "editor.rulers": [
    100
  ],
  "editor.tabSize": 2,
  "[shellscript]": {
    "editor.tabSize": 2
  },
  "[csharp]": {
    "editor.tabSize": 4
  },
  "[xml]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[ruby]": {
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.acceptSuggestionOnCommitCharacter": false,
  "vim.useSystemClipboard": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.meta": true
  },
  "workbench.editor.enablePreview": false,
  "window.zoomLevel": 0,
  "workbench.iconTheme": "vscode-icons",
  "vsicons.dontShowNewVersionMessage": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "git.ignoreLimitWarning": true,
  "editor.renderWhitespace": "all",
  "editor.autoClosingBrackets": false,
  "omnisharp.disableMSBuildDiagnosticWarning": true,
  "workbench.startupEditor": "newUntitledFile",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.formatOnType": true,
  "files.associations": {
    "*.xaml": "html"
  },
  "html.format.wrapAttributes": "force-aligned",
  "html.suggest.html5": false,
  "html.suggest.ionic": false,
  "html.validate.scripts": false,
  "html.validate.styles": false,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "window.newWindowDimensions": "maximized",
  "workbench.settings.editor": "json",
  "html.autoClosingTags": false,
  "html.suggest.angular1": false,
  "editor.parameterHints.enabled": false,
  "vsicons.projectDetection.disableDetect": true,
  "omnisharp.loggingLevel": "trace",
  "debug.internalConsoleOptions": "neverOpen",
  "vim.overrideCopy": false,
  "editor.copyWithSyntaxHighlighting": false,
  "window.nativeFullScreen": false,
  "window.restoreFullscreen": true,
  "update.channel": "none",
  "java.configuration.checkProjectSettingsExclusions": false,
  "editor.fontFamily": "FiraCode-Retina",
  "editor.fontLigatures": true,
  "dotnet-test-explorer.pathForResultFile": "/Users/harrinm/tmp",
  "csharp.referencesCodeLens.enabled": false,
  "csharp.testsCodeLens.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.views.compare.files.layout": "tree",
  "gitlens.views.compare.files.compact": true,
  "editor.detectIndentation": false,
  "python.jediEnabled": false,
  "[python]": {
    "editor.insertSpaces": true,
    "editor.detectIndentation": true,
    "editor.tabSize": 4,
    "editor.autoIndent": false
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "diffEditor.renderSideBySide": false,
  "[javascript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "extensions.ignoreRecommendations": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "html.format.indentHandlebars": true,
  "python.languageServer": "Microsoft",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "aws.telemetry": false,
  "aws.profile": "profile:cdf_dev",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.insertSpaces": true
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "editor.cursorStyle": "line",
  "editor.insertSpaces": true,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "workbench.editor.untitled.hint": "hidden",
  "security.workspace.trust.untrustedFiles": "open",
  "rest-client.enableCustomVariableReferencesCodeLens": false,
  "editor.tokenColorCustomizations": {
    "[Kibi Colors]": {
      "textMateRules": [
        {
          "scope": [
            // "keyword.operator.comparison",
          ],
          "settings": {
            // "fontStyle": "",
            // "foreground": "#FFFFFF"
            // "foreground": "#B573D3"
            // "foreground": "#EE60BF"
            // "foreground": "#FDA242"
            // "foreground": "#3DFBFE"
          }
        },
        {
          "scope": [
            // "keyword.operator.logical",
            // "keyword.operator.ternary",
            // "entity.other.attribute-name",
          ],
          "settings": {
            // "fontStyle": "",
            // "foreground": "#FFFFFF"
            // "foreground": "#B573D3"
            // "foreground": "#EE60BF"
            // "foreground": "#FDA242"
            // "foreground": "#3DFBFE"
          }
        },
      ]
    },
    "[ColorCode]": {
      "textMateRules": [
        {
          "scope": [
            "keyword"
          ],
          "settings": {
            "foreground": "#66C0FF"
          }
        },
        {
          "scope": [
            "keyword.operator.arithmetic.js"
          ],
          "settings": {
            "foreground": "#F7E36E"
          }
        },
        {
          "scope": [
            "keyword.operator.new"
          ],
          "settings": {
            "foreground": "#FF6D33",
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "variable.other.property",
            "storage.type.property.js",
            "storage.type.property.ts",
            "storage.type.property.tsx"
          ],
          "settings": {
            "foreground": "#F7E36E",
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            "storage.type.class"
          ],
          "settings": {
            "foreground": "#F55C3D",
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "keyword.other.class, entity.name.type.class"
          ],
          "settings": {
            "foreground": "#F5DA3D"
          }
        },
        {
          "scope": [
            "storage.modifier"
          ],
          "settings": {
            "foreground": "#9EFA9E"
          }
        },
        {
          "scope": [
            "constant.language"
          ],
          "settings": {
            "foreground": "#FFB699",
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            "support.type.property-name.json"
          ],
          "settings": {
            "foreground": "#66C0FF"
          }
        },
        {
          "scope": [
            "support.constant.json"
          ],
          "settings": {
            "foreground": "#F94B06"
          }
        },
        {
          "scope": [
            "meta.structure.dictionary.json meta.structure.dictionary.value constant.language"
          ],
          "settings": {
            "foreground": "#F55C3D"
          }
        }
      ],
      "comments": "#0A76C2",
      "functions": "#CCAE00",
      "keywords": "#66C0FF",
      "numbers": "#CCAE00",
      "strings": "#3DF53D",
      "types": "#0AC20A",
      "variables": "#F94B06"
    }
  },
  "workbench.colorCustomizations": {
    "[ColorCode]": {
      "foreground": "#F4B2A4",
      "editor.foreground": "#F7856E",
      "descriptionForeground": "#7D2210",
      "sideBar.foreground": "#F4B2A4",
      "input.background": "#070B0E",
      "input.foreground": "#F7856E",
      "quickInput.background": "#070B0E",
      "quickInput.foreground": "#F7856E",
      "sideBar.background": "#0D161B",
      "sideBar.border": "#070B0E",
      "editor.background": "#0D161B",
      "editorGroupHeader.tabsBackground": "#142029",
      "activityBar.background": "#070B0E",
      "tab.activeBackground": "#0D161B",
      "tab.inactiveBackground": "#070B0E",
      "terminal.background": "#070B0E",
      "selection.background": "#F4B2A4",
      "badge.background": "#142029",
      "panel.background": "#142029",
      "widget.shadow": "#192933",
      "focusBorder": "#192933"
    }
  },
